{
    "name": "creativecoder/piwik-analytics",
    "type": "project",
    "repositories": [
        {
            "type":"package",
            "package": {
                "name": "piwik/plugin-SecurityInfo",
                "version": "3.0.4",
                "source": {
                    "url": "https://github.com/piwik/plugin-SecurityInfo.git",
                    "type": "git",
                    "reference": "3.0.4"
                },
                "type": "piwik-plugin",
                "require": {
                    "composer/installers": "~1.0"
                },
                "extra": {
                    "installer-name": "SecurityInfo"
                }
            }
        }
    ],
    "require": {
        "piwik/piwik": "^3.0",
        "php": "^7.1",
        "ext-curl": "*",
        "ext-gd": "*",
        "ext-xml": "*",
        "ext-mbstring": "*",
        "piwik/plugin-SecurityInfo": "^3.0",
        "geoip2/geoip2": "^2.6"
    },
    "license": "GPL-3.0",
    "authors": [
        {
            "name": "Grant Kinney",
            "email": "hi@grant.mk"
        }
    ],
    "minimum-stability": "stable",
    "scripts": {
        "post-install-cmd": "mv plugins/* vendor/piwik/piwik/plugins/ && cp index.php vendor/piwik/piwik/ && cp config.ini.php vendor/piwik/piwik/config/"
    }
}
